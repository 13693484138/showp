page {
    background-color: #efefef;
}
.section {
    width:100%;
    padding:0 20rpx;
    background-color:#fff;
    box-sizing: border-box;
    margin-bottom: 40rpx;
}
.section:before,.section:after {
    content:'';
    display:table;
}
.cap-logistics {
    position: relative;
}
.cap-logistics:after {
    position: absolute;
    bottom:0;
    left:0;
    content: "";
    display: block;
    width:100%;
    height: 2px;
    background: -webkit-repeating-linear-gradient(135deg,#ff6c6c,#ff6c6c 20%,transparent 0,transparent 25%,#3283fa 0,#3283fa 45%,transparent 0,transparent 50%);
    background: repeating-linear-gradient(-45deg,#ff6c6c,#ff6c6c 20%,transparent 0,transparent 25%,#3283fa 0,#3283fa 45%,transparent 0,transparent 50%);
    background-size: 100px;
}
.card-icon {
    padding-left: 50rpx;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-position: left center;
    background-size: 40rpx;
}
.base-info {
    padding:20rpx 0;
    padding-right:80rpx;
    border-bottom: 1rpx dashed #ddd;
}
.base-info>view {
    padding:10rpx 0;
}
.card-images {
    padding: 30rpx 0;
}
.card-images>image {
    width:340rpx;
    margin-right:20rpx;
    height: 200rpx;
}
.tip {
    padding:30rpx 0;
}
.goods-item {
    padding:28rpx 0;
    padding-right: 60rpx;
    box-sizing: border-box;
    border-bottom: 1rpx solid #ddd;
    align-items: flex-start;
}
.cut-down {
    padding:30rpx 0;
    padding-right: 60rpx;
    
}
.goods-item:last-child {
    border-bottom: 0;
}
.goods-item-price {
    max-width: 200rpx;
    margin-left:20rpx;
}
.goods-item-info {
    align-items: flex-start;
}
.goods-item-count {
    text-align: right;
    margin-top:40rpx;
}
.goods-item-thumbnail {
    width:180rpx;
    height: 180rpx;
    margin-right:20rpx;
}
.flex-full {
    width:1;
    flex:1;
}
.flex-item {
    margin:30rpx 0;
}
.word-input {
    text-align: right;
    margin-left: 30rpx;
}
.order-total {
    padding:40rpx 0;
    text-align: right;
    border-top:1rpx dashed #ddd;

}
.footer-tab {
    border-top: 1rpx solid #ddd;
    position: fixed;
    bottom:0;
    left:0;
    width:100%;
    height: 120rpx;
    background-color: #fff;
    padding:0 20rpx;
    box-sizing: border-box;
    z-index: 9999;
}
.spacing {
    width:100%;
    height: 140rpx;
}
.c-btn {
    margin:0;
    font-size: 16px;
    background-color: #dc3c14;
    color:#fff;
}
.c-btn:after {
    margin:0;
    border-color: #dc3c14;
}
.fs-16 {
    font-size: 16px;
}
.fs-14 {
    font-size: 14px;
}
.fs-10 {
    font-size: 10px;
}
.fc-33 {
    color:#333;
}
.fc-grey {
    color:#777777
}
.fc-red {
    color:#dc3c14;
}
.border-bottom {
    border-bottom: 1rpx solid #ddd;
}