page {
    background-color: #efefef;
}
swiper {
    height:400rpx;
}
.slide-image {
    display: block;
    width:100%;
}
.active-list {
    background:#bdbdbd;
    padding:20rpx;
    padding-bottom: 0;
}
.active-item {
    width:345rpx;
    margin-right:20rpx;
    height: 200rpx;
    background-color: #fff;
    margin-bottom: 20rpx;
    border-radius: 20rpx;
    -webkit-border-radius: 20rpx;
    -moz-border-radius: 20rpx;
    -ms-border-radius: 20rpx;
    -o-border-radius: 20rpx;
    padding:20rpx;
    box-sizing: border-box;
}
.active-item:nth-child(even){
    margin-right:0;
}
.limited-section {
    background:#efefef;

}
.limited-bar {
    padding:20rpx;
    box-sizing: border-box;
}
.limited-bar-title {
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-position: left center;
    background-size: 48rpx;
    padding-left: 46rpx;
    box-sizing: border-box;
    font-size:14px;
}
.limited-bar-time>text {
    display:inline-block;
    background-color:rgb(68, 68, 68);
    margin:0 10rpx;
    color:#fff;
    font-size:12px;
    padding:10rpx;
    border-radius: 4rpx;
    -webkit-border-radius: 4rpx;
    -moz-border-radius: 4rpx;
    -ms-border-radius: 4rpx;
    -o-border-radius: 4rpx;
}
.limited-section>scroll-view {
    width:100%;
    height: 496rpx;
    white-space: nowrap;

}
.limited-bar-next {
    font-size:12px;
}
.limited-item-title {
    height: 40px;
    padding: 10rpx;
    text-align: left;
    box-sizing: border-box;
    white-space: pre-wrap
}
.limited-item {
    width:300rpx;
    height: 496rpx;
    background-color:#fff;
    border:1rpx solid #efefef;
    position: relative;
    margin-right:30rpx;
    font-size: 14px;
    display: inline-block;
    box-sizing: border-box;
    vertical-align: middle;
}

.limited-item:last-child {
    margin-right:0;
}
.price,.ori_price {
    padding:0 10rpx;
    margin:10rpx 0;
    font-size: 12px;
}
.price {
    color:#dc3c14;
    text-align: center;
}
.ori_price {
    text-align: center;
}
.limited-item-tag {
    position: absolute;
    top:0;
    right:10rpx;
    width:60rpx;
    height: 60rpx;
    background-color: #dc3c14;
    font-size: 24rpx;
    word-break: break-all;
    white-space: normal;
    color:#fff;
    text-align: center;
    line-height: 30rpx;
    padding:10rpx;
}
.limited-item-thumbnail {
    width:100%;
    height: 300rpx;
    display: block;
    max-height: 100%;
}
.limited-view-all {
 line-height: 486rpx;
 text-align: center;
 background-color:#bdbdbd;
}

.classify-section {
    background-color:#fff;
    width:100%;
    margin:40rpx 0;
}
.classify-title {
    text-align: center;
    padding: 20rpx;
    font-size: 16px;
    line-height: 14px;
    border-bottom: 1rpx solid #efefef;
}
.classify-content {
    padding:20rpx 0;
}
.classify-title>.right-arrow:after   {
    right:-40rpx;
}
.template-1 {
    height: 400rpx;
 }
.template-1-l {
    margin-left:40rpx;
    margin-right: 20rpx; 
   box-sizing: border-box;
}
.template-1-r {
    margin-right:40rpx;
    margin-left: 20rpx; 
    box-sizing: border-box;
 }
.template-1-l,.template-1-r {
    flex:1;
    position: relative;
    height: 400rpx;
}
.template-1-r>view {
    position: relative;
    width:100%;
    height: 200rpx;
    margin-bottom:20rpx;
}
.template-1-r>view:last-child {
    margin-bottom:0rpx;
}
.classify-thumbnail {
    display:block;
    width:100%;
    max-height: 100%;
    position: absolute;
    top:0;
    left:0;
    bottom:0;
    right:0;
}
.btn {
    position: absolute;
    bottom:20rpx;
    left:50%;
    transform: translate(-50%,0);
    -webkit-transform: translate(-50%,0);
    -moz-transform: translate(-50%,0);
    -ms-transform: translate(-50%,0);
    -o-transform: translate(-50%,0);
    font-size: 14px;
    text-align: center;
    width:100%;
    color:#fff;
}
.btn>.right-arrow:after {
    right:-30rpx;
    border-color: #fff;
}
.btn-radius {
    width:70%;
    background-color:#dc3c14;
    border-radius: 40rpx;
    -webkit-border-radius: 40rpx;
    -moz-border-radius: 40rpx;
    -ms-border-radius: 40rpx;
    -o-border-radius: 40rpx;
}
.btn.b-block {
    background-color:rgb(226, 171, 68);
    width:90%;
    bottom:0;
}
.template-2 {
    padding:0 30rpx;
}
.template-2-thumbnail {
    position: relative;
    width:200rpx;
    height: 200rpx;
    margin-right:20rpx;
}
.template-2-content {
    flex:1;
    width:0;
}
.template-2-title {
    font-size: 16px;
    color: #333;
}
.template-2-remark {
    font-size: 12px;
    color: rgb(44, 44, 44);
    margin:20rpx 0;
}
.template-2-price {
    font-size: 14px;
    color:#dc3c14;
}