page {
    background-color: #efefef;
    height: 100%;
}
.section {
    height: 100%;
}
.scroll-cls {
    padding:0 20rpx;
    box-sizing: border-box;
    width:100%;
    white-space: nowrap;
    background-color: #fff;
    position: relative;
    
}
.scroll-item {
    display:inline-block;
    padding: 0rpx 30rpx;
    font-size:16px;
    color:#333;
    height: 120rpx;
    line-height: 120rpx;
}
.scroll-item.active {
    color:#dc3c14;
}
.slide-line {
    position: absolute;
    top:116rpx;
    left:0;
    height: 4rpx;
    background-color:#dc3c14;
    width:30px;
    transition: all linear 320ms;
    -webkit-transition: all linear 320ms;
    -moz-transition: all linear 320ms;
    -ms-transition: all linear 320ms;
    -o-transition: all linear 320ms;
}
.listTitle {
    background-color:#fff;
    width:100%;
    margin-top:40rpx;
    height: 100rpx;
    
}
.title-item {
    font-size: 14px;
    color:#333;
    height: 100rpx;
    line-height: 100rpx;
    flex:1;
    width:0;
    text-align: center;
    box-sizing: border-box;
}
.title-item.active {
    color:#dc3c14;
}
.sort {
    height: 100%;
    width:20rpx;
    margin-left:10rpx
}
.sort-up,.sort-down {
    width:100%;
    flex:1;
    background-repeat: no-repeat;
    background-size: 40rpx;
}
.sort-down {
    background-position: center -10rpx;
}
.sort-up {
    background-position: center 28rpx;
}
.sort-asc>.sort-up {
    background-image: url('data:image/png;base64,')
}
.sort-desc>.sort-up {
    background-image: url('data:image/png;base64,')
    
}
.sort-asc>.sort-down {
    background-image: url('data:image/png;base64,')
}
.sort-desc>.sort-down {
    background-image: url('data:image/png;base64,')
}
.goods-scroll-view {
    flex:1;
    height: 0;
    background-color: #fff;
}
.goods-list {
    width:100%;
    border-top:1rpx solid #ddd;
}
.goods-item {
    float:left;
    width:50%;
    box-sizing: border-box;
    border-bottom: 1rpx solid #ddd;
    border-right: 1rpx solid #ddd;
    padding:20rpx;
}
.goods-item:nth-child(even){
    border-right: 0;
}
.item-thumbnail {
    width:100%;
    position: relative;
    height: 400rpx;
}
.item-thumbnail>.goods-thumbnail {
    width:100%;
    height: 360rpx;
    margin-top:20rpx;
}
.item-tag {
    position: absolute;
    top:0;
    right:10rpx;
    width:60rpx;
    height: 60rpx;
    background-color: #0a1af3;
    font-size: 24rpx;
    word-break: break-all;
    white-space: normal;
    color:#fff;
    text-align: center;
    line-height: 30rpx;
    padding:10rpx;
}
.item-tag:after {
    content:'';
    display: block;
    width:70rpx;
    height: 70rpx;
    position: absolute;
    top:5rpx;
    left:5rpx;
    border:1rpx solid rgba(255, 255, 255,.5);
}
.goods-name {
    font-size:14px;
    line-height: 20px;
    height: 40px;
}
.goods-price {
    color:#dc3c14;
    font-size: 16px;
    margin-top:20rpx;
}
.scroll-top {
    width:120rpx;
    height: 120rpx;
    border-radius: 100%;
    -webkit-border-radius: 100%;
    -moz-border-radius: 100%;
    -ms-border-radius: 100%;
    -o-border-radius: 100%;
    position: fixed;
    bottom:60rpx;
    right:40rpx;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: 80rpx;
    background-position: center;
    z-index: 9999;
    background-color: rgb(102, 102, 102)
}