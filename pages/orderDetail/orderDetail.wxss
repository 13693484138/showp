page {
    background-color: #efefef;
}
.section {
    background-color: #fff;
    border-top:1rpx solid #ddd;
    border-bottom: 1rpx solid #ddd;
    padding:0 20rpx;
    margin-bottom:40rpx;
}
.icon-address {
    width:40rpx;
    height: 40rpx;
    background-size: 40rpx;
    background-repeat: no-repeat;
    background-position: center;
    margin-right:20rpx;
    background-image: url('data:image/png;base64,')
}
.address-info {
    flex:1;
    width: 0;
    line-height: 30px;
}
.address-info>view {
    line-height: 20px;
    margin-bottom:20rpx;
}
.address-info>view:last-child {
    margin-bottom: 0;
}
.address-pannel {
    border-bottom: 1rpx solid #ddd;
    font-size:14px;
    color:#333;
    padding:20rpx 0;
}
.data-section {
    font-size: 14px;
    color:#666;
    padding:30rpx 0;
}
.services {
    padding:30rpx 0;
}
.service {
    flex:1;
    width:0;
    font-size: 14px;

}
.service-online {
    border-right: 1rpx solid #ddd;
}
.service >image {
    width:40rpx;
    height: 40rpx;
    margin-right: 10rpx;
}
.goods-list {
    background-color: #fff;
}
.goods-item {
    background-color: #ececec;
    padding:20rpx;
    font-size: 14px;
    margin-bottom: 20rpx;
}
.goods-item:last-child {
    margin-bottom: 0;
}
.section-list {
    background-color: #fff;
    margin-bottom: 20rpx;
}
.list-header {
    padding:20rpx;
    font-size:14px;
}
.goods-item-thumbnail {
    width:160rpx;
    height: 160rpx;
    margin-right:20rpx;
    border:1rpx solid #ddd;
}
.goods-item-info {
    flex:1;
    width:0;
}
.goods-item-info>.flex-row {
    align-items: flex-start;
}
.goods-item-name {
    flex:1;
    width:0;
    margin-right:20rpx;
}
.goods-item-count {
    text-align: right;
    margin-top: 40rpx;
    color:#555;
}
.list-content-infos {
    background-color: #fff;
    font-size: 14px;
    padding:0rpx 20rpx;
    color:#555;
}
.list-content-info {
    padding:30rpx 0;
}
.fs-red {
    color:#dc3c14;
}
.list-footer  {
    text-align: right;
    font-size: 16px;
    padding:40rpx 20rpx;
    border-top: 1rpx dashed #ddd;

}
.order-item {
    padding:40rpx 0;
    font-size: 14px;
    color:#333
}
.copy-btn {
    padding:10rpx 20rpx;
    border-radius: 10rpx;
    -webkit-border-radius: 10rpx;
    -moz-border-radius: 10rpx;
    -ms-border-radius: 10rpx;
    -o-border-radius: 10rpx;
    border:1rpx solid #ddd;
}