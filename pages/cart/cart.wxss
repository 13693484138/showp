page {
    background-color: #efefef;
}

.services {
    padding:20rpx 10rpx;
    padding-left:30rpx;
    background-image: url('data:image/png;base64,');
    background-position: left center;
    background-repeat: no-repeat;
    background-size: 30rpx;
    font-size: 12px;
    color:#dc3c14;
}
.cart-footer-bar {
    position: fixed;
    bottom:0;
    left:0;
    width:100%;
    height: 120rpx;
    background-color: #dadada;
    box-sizing: border-box;
}
.cart-footer-checkgroup {
    padding:20rpx;
    font-size: 14px;
    color:#333;
}
.btn-footer-price {
    color:#dc3c14;
    font-size: 14px;
}
.btn {
    height: 120rpx;
    line-height: 120rpx;
    border-radius: 0;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    -ms-border-radius: 0;
    -o-border-radius: 0;
    border-width: 0;
    width:180rpx;
    background-color: #dc3c14;
    color:#fff;
    margin-left: 10rpx;
}
.btn:after {
    border-width: 0;
    border-radius: 0;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    -ms-border-radius: 0;
    -o-border-radius: 0;
}
.btn[disabled] {
    background-color: #da6b4f;
    color:rgb(196, 40, 40);
}
.btn[disabled]:after {
    background-color: rgba(192, 49, 13, 0.5);
    color:rgb(250, 28, 28);
}
.gift-section {
    background-color:#fff;
}
.gift-info {
    flex:1;
    width:0;
}
.gift-info {
    align-items: flex-start;
    padding:10rpx 30rpx 10rpx 10rpx;
}
.tag {
    width:40px;
    border:1rpx solid #da6b4f;
    color:#da6b4f;
    font-size: 10px;
    line-height: 20px;
    text-align: center;
}
.view-gift {
    margin:20rpx 0;
    width:200rpx;
    text-align: center;
    font-size: 14px;
    color:#333;
    border-left:1rpx solid #ddd;

}
.gift-text {
    flex: 1;
    width:0;
    font-size: 12px;
    margin-left:10rpx;
    padding-right:30rpx;
    box-sizing: border-box;
}