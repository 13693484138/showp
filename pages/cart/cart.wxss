.list-item {
    background-color:#fff;
    padding:20rpx;
    font-size:14px;
}
.item-checkbox {
    display: block;
    margin-right:20rpx;
    position: relative;
}
.thumbnail>.status {
    position: absolute;
    bottom:0;
    left:0;
    width:100%;
    padding:10rpx;
    box-sizing: border-box;
    text-align: center;
    font-size:20rpx;
    background-color:rgba(0, 0, 0, .5);
    color:#fff;
}
.item-checkbox>checkbox {
    transform: scale(.8);
    -webkit-transform: scale(.8);
    -moz-transform: scale(.8);
    -ms-transform: scale(.8);
    -o-transform: scale(.8);
}
.item-checkbox>checkbox:after {
    position: absolute;
    content:'';
    display:block;
    width:80rpx;
    height: 80rpx;
    background-color:#fff;
    border:1rpx solid #ddd;
    top:50%;
    left:50%;
    transform:translate(-55%,-50%) scale(.8);
    z-index: 99;
    border-radius: 100%;
    -webkit-border-radius: 100%;
    -moz-border-radius: 100%;
    -ms-border-radius: 100%;
    -o-border-radius: 100%;
    -webkit-transform:translate(-55%,-50%) scale(.8);
    -moz-transform:translate(-55%,-50%) scale(.8);
    -ms-transform:translate(-55%,-50%) scale(.8);
    -o-transform:translate(-55%,-50%) scale(.8);
}
.item-checkbox>checkbox[checked='']:after {
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: 90rpx;
    background-position: center;
}
